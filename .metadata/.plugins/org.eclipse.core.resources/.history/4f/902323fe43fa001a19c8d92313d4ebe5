
//数据类型举例
public class DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 字符型
		char sName1 = '慕';
		char sName2 = '课';
		char sName3 = '中';
		char sName4 = '国';

		// 整型
		byte x = 100;
		short y = 500;
		long z = 100;
		int n = 100;

		// 浮点型
		float a = 12.34F;
		double b = 25.697;
		double c = 10;

		String aa;
		// 输出显示
		System.out.println("学习网站名为：" + sName1 + sName2 + sName3 + sName4 + aa);
		System.out.println("相应的整型数值为：x= " + x + ", y= " + y + ", z= " + z + " , n= " + n);
		System.out.println("浮点数a和c的乘积为:  " + a * c + "；浮点数b和c的乘积为:  " + b * c);

	}

}
