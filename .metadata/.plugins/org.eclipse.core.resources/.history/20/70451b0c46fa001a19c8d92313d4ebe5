package test;

public class index {
	public static void main(String[] args) {
//		2、分别定义int、short、float、double类型的变量，并赋值输出。
		
//		3、编写程序计算1到100的累加和。求和
        int i;
        int sum = 0;
        for(i = 1; i <= 100; i++) {
        	sum += i;
        }
        System.out.print("1到100的累加和=" + sum);
    }
}
