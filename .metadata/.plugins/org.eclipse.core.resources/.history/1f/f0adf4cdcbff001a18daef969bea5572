package dome926;

public class forTest {
	public static void main(String[] ags) {
//		break/continue语句练习；计算1^2+2^2+3^2+...+i^2...，结果大于5000时停止计算，输出结果和对应的i值。
		int i , k = 0;
		for (i = 0; i < 10; i++) {
			if(k > 5000) {
				break;
			}
			k = k + i * i;
			System.out.println(k + "//" + i);
		}
	}
}
